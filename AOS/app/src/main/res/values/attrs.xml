<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SeatCustomView">
        <attr name="mode" format="enum">
            <enum name="selectable" value="0" />
            <enum name="read_only" value="1" />
        </attr>
        <attr name="seatsCntPerRow" format="integer" />
        <attr name="backSeatsCnt" format="integer" />
        <attr name="totalRows" format="integer" />
        <attr name="mySeatNum" format="integer" />
        <attr name="soldOutSeatNums" format="string" />
    </declare-styleable>
</resources>